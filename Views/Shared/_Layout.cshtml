<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MY APP NAME</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.1/css/all.css" integrity="sha384-fnmOCqbTlWIlj8LyTjo7mOUStjsKC4pOpQbqyi7RrhN7udi9RwhKkMHpvLbHG9Sr" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.7.2/animate.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-select@1.13.9/dist/css/bootstrap-select.min.css">
    <link rel="stylesheet" type="text/css" href="~/vendor/animate/animate.css" />

    <link rel="stylesheet"
          type="text/css"
          href="~/fonts/font-awesome-4.7.0/css/font-awesome.min.css" />

    <!--===============================================================================================-->
    <link rel="stylesheet"
          type="text/css"
          href="~/vendor/css-hamburgers/hamburgers.min.css" />
    <!--===============================================================================================-->
    <link rel="stylesheet"
          type="text/css"
          href="~/vendor/animsition/css/animsition.min.css" />
    <!--===============================================================================================-->
    <link rel="stylesheet"
          type="text/css"
          href="~/vendor/select2/select2.min.css" />
    <!--===============================================================================================-->
    <link rel="stylesheet"
          type="text/css"
          href="~/vendor/daterangepicker/daterangepicker.css" />
    <!--===============================================================================================-->

    <link rel="stylesheet" type="text/css" href="~/css/main.css" />
    <!--===============================================================================================-->

</head>
<body>

    <div id="loader">
        <div id="loading-bar-spinner" class="spinner"><div class="spinner-icon"></div></div>
        <div class="processing"><span>sending...</span></div>
    </div>

    <!-- Top -->
    <div class="container-fluid banner">
        <nav class="navbar navbar-expand-lg navbar-light bg-transparent">
            <a class="navbar-brand">
                <div>
                    <img src="~/images/logo1.png" id="logo1" />
                    <img src="~/images/logo2.png" id="logo2" />
                    <img src="~/images/logoxxl.png" id="logo3" />
                </div>
            </a>
        </nav>



        <div class="row justify-content-center">
            <div class="col-sm-12 col-md-7 col-lg-8 col-xl-9" id="img1">
                <img src="~/images/RestoMockup.jpg" id="bigImg" />
            </div>
            <div class="col-12 col-sm-12 col-md-4 col-lg-4 col-xl-3">
                <div class="container plan">

                    <span class="Header1">YOUR RESTAURANT STORE UNDER $500</span>
                    <br />
                    <br />
                    <br />
                    <div class="row" style="padding:10px;">
                        <span class="Header2">RESPONSIVE DESIGN</span>
                        <br />
                        <br />
                        <span class="Header3">ASP.NET CORE 3.0</span>
                        <br />
                        <br />
                        <span class="Header4">CLEAN UI/UX </span>
                        <br />
                        <br />
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div>
        @RenderBody()
    </div>

    <div id="map"></div>


    <footer class="footer">
        <div class="container">
            <div class="row justify-content-between">
                <div class="col-12 col-sm-12 col-md-4 col-lg-2 col-xl-2 text-center">
                    <span class="Header8">Cherkasy, Shevchenka Blvd, 242/1</span>
                </div>
                <div class="col-12 col-sm-12 col-md-4 col-lg-3 col-xl-3 text-center">
                    <a href="https://twitter.com/royalr_twit" onmouseover="bounceEffecttwt()" style="margin-left:8px;text-decoration:none">
                        <i class="fab fa-twitter"></i>
                    </a>
                    <a href="https://twitter.com/royalr_twit" onmouseover="bounceEffectlink()" style="margin-left:8px;text-decoration:none">
                        <i class="fab fa-linkedin-in"></i>
                    </a>
                    <a href="https://twitter.com/royalr_twit" onmouseover="bounceEffectInsta()" style="margin-left:8px;text-decoration:none">
                        <i class="fab fa-instagram"></i>
                    </a>
                    <a href="https://twitter.com/royalr_twit" onmouseover="bounceEffectPint()" style="margin-left:8px;text-decoration:none">
                        <i class="fab fa-pinterest"></i>
                    </a>
                    <a href="https://twitter.com/royalr_twit" onmouseover="bounceEffectFab()" style="margin-left:8px;text-decoration:none">
                        <i class="fab fa-facebook-square"></i>
                    </a>
                </div>
                <div class="col-12 col-sm-12 col-md-4 col-lg-2 col-xl-2 text-center text-muted">
                    <span> &copy; @DateTime.Now.Year - All Right Reserved</span>
                </div>
            </div>
        </div>
    </footer>

    <script src="~/js/map.js"></script>
    <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyC34vWHbKmM-epgFUz5bIrpwQ5VSuhweZU&callback=initMap"></script>
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.6/umd/popper.min.js" integrity="sha384-wHAiFfRlMFy6i5SRaxvfOCifBUQy1xHdJ/yoi7FRNXMRBu5WHdZYu1hA6ZOblgut" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/js/bootstrap.min.js" integrity="sha384-B0UglyR+jN6CkvvICOB2joaf5I4l3gm9GU6Hc1og6Ls7i6U/mkkaduKaBhlAXv9k" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap-select@1.13.9/dist/js/bootstrap-select.min.js"></script>
    <script src="~/lib/bounce.js/bounce.js"></script>
    <script src="~/lib/jquery-ajax-unobtrusive/dist/jquery.unobtrusive-ajax.js" asp-append-version="true"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/vendor/animsition/js/animsition.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/vendor/select2/select2.min.js"></script>
    <script>
        $(".selection-2").select2({
            minimumResultsForSearch: 20,
            dropdownParent: $("#dropDownSelect1")
        });

        function succeed() {

            $('#success').show();
        }
    </script>
    <!--===============================================================================================-->
    <script src="~/vendor/daterangepicker/moment.min.js"></script>
    <script src="vendor/daterangepicker/daterangepicker.js"></script>
    <!--===============================================================================================-->
    <script src="~/vendor/countdowntime/countdowntime.js"></script>
    <!--===============================================================================================-->
    <script src="~/js/main.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
